{
	"jobtype": "IMPORT",
	"collection": "uesio/docs.lesson",
	"filetype": "CSV",
	"upsertkey": "uesio/core.id",
	"upserttemplate": "${uesio/docs.slug}_${uesio/docs.track}",
	"mappings": {
		"uesio/docs.article": {
			"matchfield": "uesio/cms.slug",
			"columnname": "article"
		},
		"uesio/docs.track": {
			"columnname": "track"
		},
		"uesio/docs.slug": {
			"columnname": "slug"
		},
		"uesio/docs.order": {
			"columnname": "order"
		}
	}
}
