<!DOCTYPE html>
<html lang="en">
    <head>
        <title>My view - Uesio</title>
        <meta charset="utf-8" />
        <meta name="viewport" content="minimum-scale=1, initial-scale=1, width=device-width, shrink-to-fit=no" />
        <style>
            {{template "fonts.css"}}
        </style>
        {{- if .DevMode -}}
        <script src="/static/vendor/react.development.js"></script>
        <script src="/static/vendor/react-dom.development.js"></script>
        {{- else -}}
        <script src="/static/vendor/react.production.min.js"></script>
        <script src="/static/vendor/react-dom.production.min.js"></script>
        {{- end -}}
        <script src="/static/vendor/emotion-css.umd.min.js"></script>
        <script src="/static/ui/uesio.js" type="module"></script>
    </head>
<body>
    <div id="root"></div>
    <script type="module">
        import * as uesio from "/static/ui/uesio.js"
        window.uesio = uesio
    </script>
    {{$workspace := .Route.Workspace -}}
    {{$buildMode := .BuildMode -}}
    {{- if .PreloadMetadata -}}
        {{- if .PreloadMetadata.ComponentPack -}}
            {{- range .PreloadMetadata.ComponentPack.IDs -}}
                {{- $packURL := getPackURL . $workspace false -}}
                {{- if $packURL -}}
                <script src="{{$packURL}}" type="module"></script>
                {{- end -}}
            {{- end -}}
            {{- if $buildMode -}}
                {{- range .PreloadMetadata.ComponentPack.IDs -}}
                    {{- $packURL := getPackURL . $workspace true -}}
                    {{- if $packURL -}}
                    <script src="{{$packURL}}" type="module"></script>
                    {{- end -}}
                {{- end -}}
            {{- end -}}
        {{- end -}}
    {{- end -}}
    <script type="module">
        uesio.loader(document.querySelector("#root"), {{.}});
    </script>
</body>
</html>
