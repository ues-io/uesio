{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "outDir": "../../dist/out-tsc",
    "module": "commonjs",
    "types": ["cypress", "node"],
    "sourceMap": false
  },
  "include": [
    "**/*.ts",
    "**/*.mts",
    "**/*.cts",
    "**/*.js",
    "**/*.mjs",
    "**/*.cjs",
    "cypress.config.ts",
    "**/*.cy.ts",
    "**/*.cy.tsx",
    "**/*.cy.js",
    "**/*.cy.jsx",
    "**/*.d.ts"
  ]
}
