          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/lazymonaco/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx webpack --config webpack.config.js",
            "cwd": "libs/lazymonaco"
          }
        }
      }
    },
    "yaml": {
      "root": "libs/yaml",
      "sourceRoot": "libs/yaml/src",
