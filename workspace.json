          "outputs": [
            "dist/ui"
          ],
          "options": {
            "command": "esbuild ./src/index.ts --bundle --outfile=../../dist/ui/uesio.js --format=esm --define:process.env.NODE_ENV=\\\"development\\\" --inject:./src/react-shim.ts --sourcemap --watch",
