    "cli": {
      "root": "apps/cli",
      "sourceRoot": "apps/cli/src",
      "projectType": "application",
      "prefix": "cli",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/cli"
          ],
          "options": {
            "commands": [
              "npx tsc",
              "mkdir -p ../../dist/cli",
              "cp ./publish/package.json ../../dist/cli/package.json"
            ],
            "parallel": false,
            "cwd": "apps/cli"
          }
        },
        "watch": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/cli"
          ],
          "options": {
            "command": "npx -w tsc &",
            "cwd": "apps/cli"
          }
        },
        "publish": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/cli"
          ],
          "options": {
            "commands": [
              "cd publish && npm version --no-git-tag-version patch",
              "mkdir -p ../../dist/cli/bin",
              "cp ./publish/package.json ../../dist/cli/package.json",
              "cp ./bin/run ../../dist/cli/bin/run",
              "cp ./bin/run.cmd ../../dist/cli/bin/run.cmd"
            ],
            "parallel": false,
            "cwd": "apps/cli"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/cli/**/*.ts*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "echo TODO: set up test for cli.",
            "cwd": "apps/cli"
          }
        }
      },
      "tags": [],
      "implicitDependencies": []
    },
