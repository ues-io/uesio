{
  "version": 1,
  "projects": {
    "platform": {
      "projectType": "application",
      "root": "apps/platform",
      "sourceRoot": "apps/platform/src",
      "architect": {
        "build": {
          "builder": "@nx-go/nx-go:build",
          "options": {
            "outputPath": "dist/apps/platform",
            "main": "apps/platform/src/main.go"
          }
        },
        "serve": {
          "builder": "@nx-go/nx-go:serve",
          "options": {
            "main": "apps/platform/src/main.go"
          }
        },
        "test": {
          "builder": "@nx-go/nx-go:test",
          "options": {
            "main": "apps/platform/src/main_test.go"
          }
        },
        "lint": {
          "builder": "@nx-go/nx-go:lint"
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
              "command": "npx webpack --config webpack.config.js",
              "cwd": "libs/ui"
          }
        }
      }
    },
    "buildtime": {
      "root": "libs/buildtime",
      "sourceRoot": "libs/buildtime/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/buildtime/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/buildtime/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
              "command": "npx webpack --config webpack.config.js",
              "cwd": "libs/buildtime"
          }
        }
      }
    },
    "apps-uesio": {
