    "uesioapps-sample": {
      "root": "libs/uesioapps/sample",
      "sourceRoot": "libs/uesioapps/sample/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/uesioapps/sample/**/*.ts"]
          }
        }
      }
    },
    "uesioapps-uesio": {
      "root": "libs/uesioapps/uesio",
      "sourceRoot": "libs/uesioapps/uesio/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
