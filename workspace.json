{
  "version": 1,
  "projects": {
    "platform": {
      "projectType": "application",
      "root": "apps/platform",
      "sourceRoot": "apps/platform",
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/platform"
          ],
          "options": {
            "commands": [
              "go build -o ../../dist/platform/uesio"
            ],
            "cwd": "apps/platform"
          }
        },
        "watch": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx webpack --watch --config webpack.config.js &",
            "cwd": "apps/platform"
          }
        },
        "lint": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "test -z $(gofmt -l ./pkg/**)",
            "cwd": "apps/platform"
          }
        },
        "seed": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "../../dist/platform/uesio seed",
            "cwd": "apps/platform"
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "../../dist/platform/uesio serve",
            "cwd": "apps/platform"
          }
        },
        "build-image": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "docker build --tag uesio:latest -f ./apps/platform/Dockerfile .",
            "cwd": "."
          }
        },
        "test": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "go test -v ./...",
            "cwd": "apps/platform"
          }
        }
      }
    },
