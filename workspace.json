          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/apps/uesio/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "apps-sample": {
      "root": "libs/apps/sample",
      "sourceRoot": "libs/apps/sample/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
