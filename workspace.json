{
  "version": 1,
  "projects": {
    "platform": {
      "projectType": "application",
      "root": "apps/platform",
      "sourceRoot": "apps/platform/src",
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "go build -o ../../dist/platform/uesio",
            "cwd": "apps/platform"
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "../../dist/platform/uesio serve",
            "cwd": "apps/platform"
          }
        },
        "seed": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "../../dist/platform/uesio seed",
            "cwd": "apps/platform"
          }
        },
        "test": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "echo todo",
            "cwd": "apps/platform"
          }
        },
        "lint": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "echo todo",
            "cwd": "apps/platform"
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx webpack --config webpack.config.js",
            "cwd": "libs/ui"
          }
        }
      }
    },
