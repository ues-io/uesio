      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/ui"
          ],
          "options": {
            "command": "esbuild ./src/index.ts --bundle --outfile=../../dist/ui/uesio.js --format=esm --inject:./src/react-shim.ts --minify --sourcemap",
