    "vendor": {
      "root": "libs/vendor",
      "sourceRoot": "libs/vendor/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "mkdir -p dist/vendor",
              "wget -O dist/vendor/react.development.js https://unpkg.com/react@16/umd/react.development.js",
              "wget -O dist/vendor/react.production.min.js https://unpkg.com/react@16/umd/react.production.min.js",
              "wget -O dist/vendor/react-dom.development.js https://unpkg.com/react-dom@16/umd/react-dom.development.js",
              "wget -O dist/vendor/react-dom.production.min.js https://unpkg.com/react-dom@16/umd/react-dom.production.min.js",
              "wget -O dist/vendor/redux.js https://cdnjs.cloudflare.com/ajax/libs/redux/4.0.4/redux.js",
              "wget -O dist/vendor/react-redux.js https://cdnjs.cloudflare.com/ajax/libs/react-redux/7.1.3/react-redux.js",
              "wget -O dist/vendor/material-ui.development.js https://unpkg.com/@material-ui/core@latest/umd/material-ui.development.js",
              "echo Done!"
            ],
            "parallel": false
          }
        }
      }
    },
    "cli": {
      "root": "apps/cli",
      "sourceRoot": "apps/cli/src",
      "projectType": "application",
      "prefix": "cli",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx tsc",
            "cwd": "apps/cli"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cli/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/cli/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "cli"
}
