 "scripts": {
  "dev": "npm run kill:webserver && npm run start & npm run kill:firestore && firebase emulators:start & npm run build-all && npm run nx -- seed platform && npm run dev:mock:crm && npm run dev:uesio:login && npm run deploy:uesioapps && npm run watch-all &",
  "watch-all": "nx run-many --target=watch --parallel --all",
  "kill:webserver": "lsof -t -i :3000 | xargs kill -9",
  "kill:firestore": "lsof -t -i :8080 -i :4000 | xargs kill -9",
  "deploy:uesioapps": "nx run-many --target=deploy --all",
  "dev:mock:crm": " cd ./libs/uesioapps/crm && ../../../apps/cli/bin/run upsert -f data/contacts.csv -c crm.contacts && cd -",
  "dev:uesio:login": "cd ./libs/uesioapps/uesio && ../../../apps/cli/bin/run login && cd -",
  "thomas:test:build-uesio": "cd ./libs/uesioapps/uesio && ../../../apps/cli/bin/run pack && cd -",
  "affected": "nx affected",
  "affected:apps": "nx affected:apps",
  "affected:build": "nx affected:build",
  "affected:dep-graph": "nx affected:dep-graph",
  "affected:e2e": "nx affected:e2e",
  "affected:libs": "nx affected:libs",
  "affected:lint": "nx affected:lint",
  "affected:test": "nx affected:test",
  "build": "nx build",
  "build-all": "npm run thomas:test:build-uesio && nx run-many --target=build --all",
  "dep-graph": "nx dep-graph",
  "e2e": "nx e2e",
  "format": "nx format:write",
  "format:check": "nx format:check",
  "format:write": "nx format:write",
  "help": "nx help",
  "lint": "nx workspace-lint && nx lint",
  "lint-all": "nx run-many --target=lint --all",
  "nx": "nx",
  "setup-ssl": "cd ./apps/platform/ssl && ./create.sh",
  "start": "nx serve platform",
  "test": "nx test",
  "update": "nx migrate latest",
  "workspace-schematic": "nx workspace-schematic",
  "uesio": "WD=$PWD && cd $INIT_CWD && $WD/apps/cli/bin/run"
 },
