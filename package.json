{
  "name": "uesio",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "get-module-version": "node -p \"require('./package').devDependencies['$npm_config_modulename']\" | sed 's/^\\^//g'",
    "clean": "rm -rf dist; rm -rf node_modules/.cache/nx",
    "dev": "npm run clean; npm run kill:webserver; npm run kill:firestore; npm run build-all; npm run start & firebase emulators:start & npm run nx -- seed platform; npm run dev:mock:crm; npm run dev:uesio:login; npm run deploy:uesioapps; npm run watch-all &",
    "watch-all": "nx run-many --target=watch --parallel --all",
    "kill:webserver": "lsof -t -i :3000 | xargs kill -9",
    "kill:firestore": "lsof -t -i :8080 -i :4000 | xargs kill -9",
    "deploy:uesioapps": "nx run-many --target=deploy --all",
    "dev:mock:crm": "cd ./libs/uesioapps/crm && ../../../apps/cli/bin/run work dev && ../../../apps/cli/bin/run upsert -f data/contacts.csv -c crm.contacts && cd -",
    "dev:uesio:login": "cd ./libs/uesioapps/uesio && ../../../apps/cli/bin/run login && cd -",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "nx build",
    "build-all": "nx run-many --target=build --all",
    "build-all-ignore-cache": "nx run-many --target=build --all --skip-nx-cache",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "prettier-check": "prettier --check .",
    "prettier-format": "prettier --write .",
    "lint": "nx workspace-lint && nx lint",
    "lint-all": "nx run-many --target=lint --all",
    "nx": "nx",
    "setup-ssl": "cd ./apps/platform/ssl && ./create.sh",
    "start": "nx serve platform",
    "test": "nx test",
    "update": "nx migrate latest",
    "uesio": "WD=$PWD && cd $INIT_CWD && $WD/apps/cli/bin/run",
    "workspace-generator": "nx workspace-generator"
  },
  "files": [
    "/apps/cli/bin",
    "/apps/cli/lib",
    "/apps/cli/npm-shrinkwrap.json",
    "/apps/cli/oclif.manifest.json"
  ],
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@nrwl/cli": "12.1.1",
    "@nrwl/eslint-plugin-nx": "12.1.1",
    "@nrwl/jest": "12.1.1",
    "@nrwl/node": "12.1.1",
    "@nrwl/tao": "12.1.1",
    "@nrwl/workspace": "12.1.1",
    "@nx-go/nx-go": "^1.7.1",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/test": "^1.2.8",
    "@popperjs/core": "^2.9.2",
    "@reduxjs/toolkit": "^1.5.1",
    "@tanem/react-nprogress": "^3.0.71",
    "@types/archiver": "^5.1.0",
    "@types/chai": "^4.2.17",
    "@types/chroma-js": "^2.1.3",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/module-alias": "^2.0.0",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-facebook-login": "^4.1.3",
    "@types/react-redux": "^7.1.16",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "amazon-cognito-identity-js": "^4.6.0",
    "archiver": "^5.3.0",
    "chai": "^4.3.4",
    "chalk": "^4.1.1",
    "chroma-js": "^2.1.1",
    "css-loader": "^5.2.4",
    "downshift": "^6.1.3",
    "eslint": "7.25.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "globby": "^11.0.3",
    "inquirer": "^8.0.0",
    "jest": "26.6.3",
    "lodash": "4.17.21",
    "monaco-editor": "^0.23.0",
    "monaco-editor-webpack-plugin": "^3.0.1",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-monaco-editor": "^0.43.0",
    "react-popper": "^2.2.5",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-thunk": "2.3.0",
    "shortid": "^2.2.16",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "26.5.5",
    "ts-loader": "^9.1.1",
    "ts-node": "9.1.1",
    "tslib": "^2.2.0",
    "tslint": "6.1.3",
    "typescript": "4.2.4",
    "webpack": "^5.36.1",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0",
    "yaml": "^1.10.2"
  },
  "bin": {
    "uesio": "/apps/cli/bin/run"
  },
  "oclif": {
    "commands": "/dist/cli/lib/apps/cli/src/commands",
    "bin": "uesio",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "dirname": "/apps/cli"
  }
}
