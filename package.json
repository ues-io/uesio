{
  "name": "uesio",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "nx build",
    "build-cli": "nx build cli",
    "build-platform-ignore-cache": "nx build platform --skip-nx-cache",
    "build-all": "nx run-many --target=build --all",
    "build-all-ignore-cache": "nx run-many --target=build --all --skip-nx-cache",
    "clean": "rm -rf dist; rm -rf node_modules/.cache/nx",
    "get-module-version": "node -p \"require('./package').devDependencies['$npm_config_modulename']\" | sed 's/^\\^//g'",
    "graceful-shutdown": "lsof -t -i :3000 | xargs kill -15",
    "kill:webserver": "lsof -t -i :3000 | xargs kill -9",
    "tests-cleanup": "bash scripts/tests-cleanup.sh",
    "tests-e2e": "bash scripts/run-e2e-tests.sh",
    "tests-init": "bash scripts/tests-init.sh",
    "tests-integration": "bash scripts/run-integration-tests.sh",
    "tests-all": "bash scripts/run-all-tests.sh",
    "tests-ci": "bash scripts/run-all-tests-ci.sh",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "format-all": "nx format:write --all",
    "format-check-all": "nx format:check --all",
    "help": "nx help",
    "in-docker": "docker compose up -d && docker-compose -f docker-compose-local.yaml up -d",
    "in-docker-force-build": "docker compose up -d && docker-compose -f docker-compose-local.yaml up -d --build",
    "lint": "nx lint",
    "lint-all": "nx run-many --target=lint --all",
    "migrations": "nx migrate-db platform",
    "migrate:create": "migrate create -dir apps/platform/migrations -ext sql -seq -digits 1",
    "nx": "nx",
    "prettier-check": "prettier --check .",
    "prettier-format": "prettier --write .",
    "seeds": "nx seed platform",
    "setup-ssl": "cd ./apps/platform/ssl && ./create.sh",
    "start": "nx serve platform",
    "test": "nx run-many --target=test --all",
    "watch-all": "nx run-many --target=watch --all --parallel=8"
  },
  "files": [],
