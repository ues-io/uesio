{
  "name": "uesio",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "get-module-version": "node -p \"require('./package').devDependencies['$npm_config_modulename']\" | sed 's/^\\^//g'",
    "clean": "rm -rf dist; rm -rf node_modules/.cache/nx",
    "watch-all": "nx run-many --target=watch --all --parallel=12",
    "kill:webserver": "lsof -t -i :3000 | xargs kill -9",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "nx build",
    "build-cli": "nx build cli",
    "build-all": "nx run-many --target=build --all",
    "build-all-ignore-cache": "nx run-many --target=build --all --skip-nx-cache",
    "tests-e2e": "bash scripts/run-e2e-tests.sh",
    "tests-integration": "bash scripts/run-integration-tests.sh",
    "tests-all": "bash scripts/run-all-tests.sh",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "in-docker": "docker compose up -d && docker-compose -f docker-compose-local.yaml up -d",
    "in-docker-force-build": "docker compose up -d && docker-compose -f docker-compose-local.yaml up -d --build",
    "prettier-check": "prettier --check .",
    "prettier-format": "prettier --write .",
    "lint": "nx workspace-lint && nx lint",
    "lint-all": "nx run-many --target=lint --all",
    "migrate:create": "migrate create -dir apps/platform/migrations -ext sql -seq -digits 1",
    "nx": "nx",
    "setup-ssl": "cd ./apps/platform/ssl && ./create.sh",
    "start": "nx serve platform",
    "test": "nx test"
  },
  "files": [],
  "devDependencies": {
    "@emotion/css": "^11.10.6",
    "@floating-ui/react": "^0.23.1",
    "@monaco-editor/react": "^4.4.6",
    "@nrwl/cli": "14.7.13",
    "@nrwl/eslint-plugin-nx": "14.7.13",
    "@nrwl/jest": "14.7.13",
    "@nrwl/linter": "14.7.13",
    "@orangeopensource/hurl": "^2.0.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@tanem/react-nprogress": "^5.0.14",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@types/chai": "^4.3.3",
    "@types/jest": "29.0.3",
    "@types/lodash": "^4.14.185",
    "@types/papaparse": "^5.3.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.24",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "chai": "^4.3.6",
    "chart.js": "^3.9.1",
    "cypress": "^12.3.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lodash": "4.17.21",
    "monaco-editor": "^0.34.0",
    "nx": "14.7.13",
    "papaparse": "^5.3.2",
    "prettier": "2.7.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^4.3.7",
    "react-markdown": "^8.0.3",
    "react-redux": "^8.0.4",
    "react-syntax-highlighter": "^15.5.0",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.4.0",
    "redux": "^4.2.0",
    "remark-gfm": "^3.0.1",
    "tailwind-merge": "^1.12.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "yaml": "2.1.1"
  }
}
