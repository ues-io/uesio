  "scripts": {
    "get-module-version": "node -p \"require('./package').devDependencies['$npm_config_modulename']\" | sed 's/^\\^//g'",
    "clean": "rm -rf dist; rm -rf node_modules/.cache/nx",
    "watch-all": "nx run-many --target=watch --all --parallel=12",
    "kill:webserver": "lsof -t -i :3000 | xargs kill -9",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "nx build",
    "build-all": "nx run-many --target=build --all",
    "build-all-ignore-cache": "nx run-many --target=build --all --skip-nx-cache",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "prettier-check": "prettier --check .",
    "prettier-format": "prettier --write .",
    "lint": "nx workspace-lint && nx lint",
    "lint-all": "nx run-many --target=lint --all",
    "nx": "nx",
    "setup-ssl": "cd ./apps/platform/ssl && ./create.sh",
    "start": "nx serve platform",
    "test": "nx test",
    "update": "nx migrate latest",
    "clio": "WD=$PWD && cd $INIT_CWD && $WD/dist/clio/clio",
    "workspace-generator": "nx workspace-generator"
  },
  "files": [],
