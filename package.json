{
  "name": "uesio",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build-cli": "nx build cli",
    "build-platform-ignore-cache": "nx build platform --skipNxCache",
    "build-all": "nx run-many --target=build --all",
    "build-all-ignore-cache": "nx run-many --target=build --all --skipNxCache",
    "clean": "rm -rf dist; nx reset",
    "get-module-version": "node -p \"require('./package').devDependencies['$npm_config_modulename']\" | sed 's/^\\^//g'",
    "graceful-shutdown": "lsof -t -i :3000 | xargs kill -15",
    "kill:webserver": "lsof -t -i :3000 | xargs kill -9",
    "tests-cleanup": "nx run platform-integration-tests:cleanup-data",
    "tests-e2e": "nx run-many -t test-e2e",
    "tests-init": "nx run platform-integration-tests:setup-data",
    "tests-integration": "nx run-many -t test-integration",
    "tests-all": "npm run test && npm run tests-integration && npm run tests-e2e",
    "tests-docker": "nx run uesio:tests-docker",
    "tests-cypress-open": "nx run platform-e2e:open-cypress",
    "tests-cypress-run": "nx run platform-e2e:e2e",
    "format": "npm run format:write",
    "format:check": "nx format:check && nx run-many -t format:check || echo \"Formatting Errors Found!\"",
    "format:write": "nx format:write && nx run-many -t format:write || echo \"Formatting Failed!\"",
    "format-all": "nx format:write --all && nx run-many -t format:write || echo \"Formatting Failed!\"",
    "format-check-all": "nx format:check --all && nx run-many -t format:check || echo \"Formatting Errors Found!\"",
    "help": "nx help",
    "in-docker": "nx run platform:serve-image",
    "start-deps": "nx run platform:start-deps",
    "lint-all": "nx run-many --target=lint --all",
    "migrations": "nx migrate-db platform",
    "migrate:create": "migrate create -dir apps/platform/migrations -ext sql -seq -digits 1",
    "nx": "nx",
    "prettier-check": "prettier --check .",
    "prettier-format": "prettier --write .",
    "typecheck-all": "nx run-many -t typecheck --all",
    "seeds": "nx seed platform",
    "setup-ssl": "nx run platform:setup-ssl",
    "setup-local-dns": "nx run uesio:setup-local-dns",
    "start": "npm run dev",
    "dev": "nx run platform:dev",
    "test": "nx run-many -t test",
    "watch-all": "npm run build-all && nx watch --all -- nx run-many -t build -p \\$NX_PROJECT_NAME",
    "watch-platform-deps": "nx run platform:watch-deps",
    "watch-dev": "nx run platform:dev --watch"
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "tests-docker": {
        "executor": "nx:run-commands",
        "options": {
          "command": "bash ./scripts/run-docker-tests.sh",
          "cwd": "{workspaceRoot}"
        }
      },
      "setup-local-dns": {
        "executor": "nx:run-commands",
        "options": {
          "command": "bash ./seed-etc-hosts.sh",
          "cwd": "{workspaceRoot}/scripts"
        }
      }
    }
  },
  "files": [],
  "devDependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@eslint/json": "^0.12.0",
    "@floating-ui/react": "^0.27.12",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@monaco-editor/react": "^4.7.0",
    "@nx/cypress": "21.1.2",
    "@nx/eslint": "21.1.2",
    "@nx/eslint-plugin": "21.1.2",
    "@nx/jest": "21.1.2",
    "@nx/js": "21.1.2",
    "@nx/react": "21.1.2",
    "@nx/workspace": "21.1.2",
    "@orangeopensource/hurl": "^6.1.1",
    "@react-hook/resize-observer": "^2.0.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@shikijs/monaco": "^3.5.0",
    "@shikijs/rehype": "^3.5.0",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "^1.11.31",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.38",
    "@tanem/react-nprogress": "^5.0.55",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@types/chai": "^5.2.2",
    "@types/hast": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/json-pointer": "^1.0.34",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.29",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-transition-group": "^4.4.12",
    "chai": "^5.2.0",
    "chart.js": "^4.4.9",
    "css-tree": "^3.1.0",
    "cypress": "^14.4.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-yml": "^1.18.0",
    "expr-eval": "^2.0.2",
    "fuzzysort": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "monaco-editor": "0.50.0",
    "nx": "21.1.2",
    "papaparse": "^5.5.3",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.6.0",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.5.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^2.6.0",
    "ts-jest": "^29.3.4",
    "ts-json-schema-generator": "^2.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.8.0"
  },
  "overrides": {
    "@twind/core": {
      "typescript": "$typescript"
    },
    "@twind/preset-autoprefix": {
      "typescript": "$typescript"
    },
    "@twind/preset-tailwind": {
      "typescript": "$typescript"
    },
    "@modern-js/node-bundle-require": {
      "esbuild": "$esbuild"
    }
  }
}
